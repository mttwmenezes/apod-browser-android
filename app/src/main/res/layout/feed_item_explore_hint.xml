<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    style="?attr/materialCardViewFilledStyle"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginHorizontal="@dimen/margin_regular"
    app:shapeAppearance="?attr/shapeAppearanceCornerLarge"
    tools:layout_marginVertical="@dimen/margin_small">

    <TextView
        android:id="@+id/explore_hint_label"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margin_regular"
        android:drawablePadding="@dimen/padding_small"
        android:text="@string/latest_feed_explore_hint"
        android:textAppearance="?textAppearanceBodyLarge"
        app:drawableStartCompat="@drawable/ic_explore" />

</com.google.android.material.card.MaterialCardView>